{
  "type": "module",
  "name": "@tomer/generator-js",
  "version": "1.0.2",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomeraberbach@gmail.com",
    "url": "https://tomeraberba.ch"
  },
  "description": "A Yeoman generator for scaffolding my JavaScript modules.",
  "keywords": [
    "node-module",
    "pnpm",
    "prettier",
    "eslint",
    "ava",
    "c8",
    "lint-staged",
    "commitlint",
    "husky",
    "yeoman-generator"
  ],
  "homepage": "https://github.com/TomerAberbach/generator-js",
  "repository": "TomerAberbach/generator-js",
  "bugs": {
    "url": "https://github.com/TomerAberbach/generator-js/issues"
  },
  "license": "MIT",
  "files": [
    "app"
  ],
  "engines": {
    "node": ">= 12.17"
  },
  "main": "app/index.js",
  "scripts": {
    "prepare": "tomer install",
    "lint": "tomer lint",
    "test": "tomer test",
    "clean": "del temp"
  },
  "prettier": "@tomer/prettier-config",
  "eslintConfig": {
    "extends": "@tomer",
    "rules": {
      "compat/compat": "off"
    }
  },
  "ava": {
    "files": [
      "!./app/templates/**/*"
    ],
    "failWithoutAssertions": false
  },
  "dependencies": {
    "@sindresorhus/slugify": "2.1.0",
    "camelcase": "6.2.0",
    "got": "11.8.2",
    "is-scoped": "3.0.0",
    "normalize-url": "7.0.0",
    "superb": "4.0.0",
    "yeoman-generator": "5.4.0"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "del": "6.0.0",
    "del-cli": "4.0.1",
    "pify": "5.0.0",
    "tomer": "0.0.3",
    "yeoman-assert": "3.1.1",
    "yeoman-environment": "3.5.1",
    "yeoman-test": "6.2.0"
  }
}
