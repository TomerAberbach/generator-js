{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },
  "description": "<%= moduleDescription %>",
  "keywords": [],
  "homepage": "https://github.com/<%= githubUsername %>/<%= unscopedModuleName %>",
  "repository": "<%= githubUsername %>/<%= unscopedModuleName %>",
  "bugs": {
    "url": "https://github.com/<%= githubUsername %>/<%= unscopedModuleName %>/issues"
  },
  "license": "<%= licenseIdentifier %>",
  "files": [
    "src"
  ],
  "type": "module",<% if (isNodeSupported) { %>
  "engines": {
    "node": ">= 12.17"
  },<% } %><% if (isBrowserSupported) { %>
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ],<% } %>
  "exports": "./src/index.js",
  "module": "./src/index.js",<%if (includesTypes) { %>
  "types": "./src/index.d.ts",<% } %>
  "scripts": {
    "prepare": "tomer install",
    "lint": "tomer lint",
    "test": "tomer test"
  }
}
