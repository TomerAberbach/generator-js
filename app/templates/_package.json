{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },
  "description": "<%= moduleDescription %>",
  "keywords": [],
  "homepage": "https://github.com/<%= githubUsername %>/<%= unscopedModuleName %>",
  "repository": "<%= githubUsername %>/<%= unscopedModuleName %>",
  "bugs": {
    "url": "https://github.com/<%= githubUsername %>/<%= unscopedModuleName %>/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/<%= githubUsername %>"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">= 22"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier --cache --write .",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/ --fix .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "bench": "vitest bench",
    "build": "tsdown"
  },
  "prettier": "@tomer/prettier-config"
}
